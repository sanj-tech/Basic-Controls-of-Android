<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.jsstech.listviewexample">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.ListViewExample">
        <activity android:name=".SpinnerActivity"
            android:label="Spinner Example"
            ></activity>
        <activity
            android:name=".MenuExample"
            android:label="Menu Example"
            android:screenOrientation="landscape" />
        <activity
            android:name=".MultiAutoCTV"
            android:label="Multi AutoComplete TextView" />
        <activity
            android:name=".AutoCompleteTextViewActivity"
            android:label="Auto Complete TextView" />
        <activity android:name=".MainActivity"
            android:label="ListView Example">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>